{
  "compilerOptions": {
    "target": "esnext",
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "noEmit": true,
    "baseUrl": ".",
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true
  },
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": false
  }
}
