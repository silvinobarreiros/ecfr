{
  "name": "api",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "sandbox": "npm run build && node dist/src/sandbox.js",
    "build": "rimraf dist && tsc --noEmit false && tsc-alias",
    "start": "node dist/src/main.js | pino-pretty",
    "dev:server": "npm run build && npm run start",
    "dev": "nodemon --watch",
    "migrations": "npx tsx src/migration.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest -c jest.unit.config.ts",
    "test:spec": "jest -c jest.spec.config.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "lint:fix": "tsc --noEmit && eslint --fix \"src/**/*.ts*\"",
    "prettier": "prettier --write \"src/**/*.{js,ts}\""
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.16.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.21.4",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pino-pretty": "^11.1.0",
    "rimraf": "^5.0.7",
    "source-map-support": "^0.5.21",
    "testcontainers": "^10.16.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.11.0",
    "typescript": "*"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "camelcase-keys": "^9.1.3",
    "constructs": "^10.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "eslint-plugin-drizzle": "^0.2.3",
    "express": "^4.19.2",
    "file-type": "^19.4.1",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.9",
    "node-fetch": "^3.3.2",
    "snakecase-keys": "^8.0.1",
    "stripe": "^17.4.0",
    "ts-results-es": "^4.2.0",
    "uuid": "^11.0.5",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  }
}
